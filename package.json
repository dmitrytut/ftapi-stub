{
  "name": "ftapi-stub",
  "description": "REST api stub",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "APP_MOCKS=1 NODE_ENV=localhost node start",
    "start-integration": "NODE_ENV=integration node start",
    "production": "NODE_ENV=production node ./.build/server.js",
    "build": "NODE_ENV=production node build",
    "prepack-app": "mkdir .docker; sed 's/\"version\":.*/\"version\": \"0.0.0-template\",/g' package.json > ./.docker/tmp-package.json && cp -rf ./docker/* ./.docker/",
    "pack-app": "node pack-app",
    "predocker-build": "npm run pack-app",
    "docker-build": "docker build -t ftapi-stub ./.docker",
    "docker-push": "docker push ufr-registry.moscow.alfaintra.net:5000/$npm_package_name:$npm_package_version",
    "release-prepatch": "npm version preptch -m 'chore(*) preptch version'",
    "release-prerelease": "npm version prerelease -m 'chore(*) prerelease version'",
    "release-patch": "npm version patch -m 'chore(*) patch version'",
    "release-minor": "npm version minor -m 'chore(*) minor version'",
    "release-major": "npm version major -m 'chore(*) major version'",
    "mocha": "mocha ./src/**/*-test.jsx",
    "karma": "karma start",
    "test": "eslint ./src/ --ext .js,.jsx --config ./node_modules/arui-feather/.eslintrc"
  },
